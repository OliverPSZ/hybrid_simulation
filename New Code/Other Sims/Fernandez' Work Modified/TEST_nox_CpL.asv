    CpL_J_kmolK(Temp_i) = MW*nox_CpL(T_Kelvin, 'J_kmolK');
    Error = abs(CpL_J_kgK(Temp_i)-
end

plot(Temp_K, CpL_J_kgK, Temp_K, CpL_J_kmolK);